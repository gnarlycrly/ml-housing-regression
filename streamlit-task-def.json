{
  "family": "housing-streamlit",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::304106858472:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::304106858472:role/ecs_housing_streamlit_s3_role",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "housing-streamlit",
      "image": "304106858472.dkr.ecr.us-west-1.amazonaws.com/housing-streamlit:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8501,
          "protocol": "tcp"
        }
      ],
          "environment": [
      {
        "name": "STREAMLIT_SERVER_PORT",
        "value": "8501"
      },
      {
        "name": "STREAMLIT_SERVER_ADDRESS",
        "value": "0.0.0.0"
      },
      {
        "name": "STREAMLIT_SERVER_BASEURLPATH",
        "value": "/dashboard"
      },
      {
        "name": "STREAMLIT_BROWSER_GATHERUSAGESTATS",
        "value": "false"
      },
      {
        "name": "API_URL",
        "value": "http://housing-api-alb-482895428.us-west-1.elb.amazonaws.com/predict"
      },
      {
        "name": "S3_BUCKET",
        "value": "ml-housing-regression-data"
      },
      {
        "name": "AWS_REGION",
        "value": "us-west-1"
      }
    ]

    }
  ]
}
